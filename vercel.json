{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/generate",
        "dest": "/api/generate.js"
      },
      {
        "src": "/validate",
        "dest": "/api/validate.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }